{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "inno-muskelanalyse",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "open": true
      },
      "protocol": {
        "asset": true,
        "assetScope": [
          "$APPDATA/micrographs/**/*"
        ]
      },
      "path": {
        "all": true
      },
      "shell": {
        "all": false,
        "open": true,
        "sidecar": true,
        "execute": true,
        "scope": [
          {
            "name": "resources/segmentation/bin/segmentation",
            "sidecar": true,
            "args": true
          },
          {
            "name": "resources/analysis/bin/analysis",
            "sidecar": true,
            "args": true
          }
        ]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [
        "resources/segmentation/bin/segmentation",
        "resources/analysis/bin/analysis"
      ],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "at.technikum-wien.inno-muskelanalyse",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "width": 800,
        "minHeight": 600,
        "minWidth": 800,
        "title": "INNO Muskelanalyse"
      }
    ]
  }
}